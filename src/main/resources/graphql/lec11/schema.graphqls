
type Query {
	search: [Result]
}

union Result = Fruit | Electronics | Book | Location


type Fruit {
	description: String
	expiryDate: String
}

type Electronics {
	id: ID!
	description: String
	price: Int
	brand: Brand
}

enum Brand {
	APPLE
	SAMSUNG
}

type Book {
	title: String
	author: String
}

type Location {
	street: String
	city: String
}
